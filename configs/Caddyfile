{
  admin off
}

oracle1.qqey.net {
  log {
    output stdout
    format console
  }

  handle /vmess* {
    reverse_proxy v2ray:443 {
      header_up Host {http.request.host}
      header_up X-Real-IP {http.request.remote}
      header_up X-Forwarded-Proto {http.request.scheme}
      header_up -Origin
    }
  }

  handle {
    root * /srv/web
    file_server
  }
}
